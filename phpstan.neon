parameters:

    paths:
        - src

    level: 6

    reportUnmatchedIgnoredErrors: false

    ignoreErrors:
        #- '#Parameter .* expects .*, Illuminate\\Database\\Eloquent\\Model given#'
        #- '#Access to an undefined property App\\Models\\*#'
